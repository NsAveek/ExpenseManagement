<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorSelector">
        <attr name="colors" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ColorDialView">
        <attr name="colors"/>
        <attr name="tickRadius" format="dimension"></attr>
        <attr name="tickPadding" format="dimension"></attr>
        <attr name="dialDiameter" format="dimension"></attr>
        <attr name="scaleToFit" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="CustomButtonShapeView">
        <attr name="scaleToFitXY" format="boolean"/>
        <attr name="color" format="color"/>
    </declare-styleable>
</resources>
